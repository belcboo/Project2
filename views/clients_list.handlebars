 <div class="container">
   <!-- SUB MENU -->
   <div class="row mt-2 mb-2">
     <div class="col-sm-12">
       <ul class="nav nav-pills justify-content-center">
         <li class="nav-item">
           <a class="nav-link active btn-dark" href="/client/list">CLIENT LIST</a>
         </li>
         <li class="nav-item">
           <a class="nav-link disabled" href="/client/new">NEW CLIENT</a>
         </li>
       </ul>
     </div>
   </div>
  <hr>

                        <div class="row">
                            <div class="col-lg-12">
                                <label for="inputType" class= "justify-content-center" class="col-md-10 control-label">Client Search</label>
                                    <input class="col-md-10" type="text" id="clientSearch" onkeyup="search()" placeholder="Search for names.." title="Type in a name">
                                </div>
                            </div>
                        </div>

                        <div class="container">

                            <div>

                                <table id="clientTable" class="table table-striped">

                                    <thead>

                                        <tr>

                                            <th>Client ID#</th>

                                            <th>Client Name</th>

                                            <th>Client Company</th>

                                            <th>Client Email</th>

                                            <th>Client Phone</th>

                                        </tr>

                                    </thead>

                                    <tbody>

                                        <div>

                                            {{#each client}}

                                            <tr>

                                                <td>{{this.client_id}}</td>

                                                <td>{{this.client_name}}</td>

                                                <td>{{this.client_company}}</td>

                                                <td>{{this.client_email}}</td>

                                                <td>{{this.client_phone}}</td>

                                            </tr>

                                            {{/each}}

                                        </div>

                                    </tbody>

                                </table>
                                
                            </div>

                        </div>

                    </div>


                    <!-- *******************************************************
              
                           YOUR CODE FOR THE BODY OF THE HANDLEBAR SHOULD END HERE.
              
                        ******************************************************** -->

                </div>

            </div>

        </div>

    </div>

</div>

