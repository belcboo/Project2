<div class="container">

        <div class="row">

            <div class="col-lg-12">

                <div class="feature title-page text-left" style="padding-bottom: 10px;">

                    <h2 class="color-a">Client List</h2>

                </div>

                <!-- end build -->

            </div>

            <!-- end col -->

        </div>

        <div class="container">

                <div>

                    <table class="table table-striped">

                        <thead>

                            <tr>

                                <td>Client ID#</td>

                                <td>Client Name</td>

                                <td>Client Company</td>

                                <td>Client Email</td>

                                <td>Client Phone</td>

                            </tr>

                        </thead>

                        <tbody>

                            <div>

                                {{#each clients}}

                                <tr>

                                    <td>{{clients.client_id}}</td>

                                    <td>{{clients.client_name}}</td>

                                    <td>{{clients.client_company}}</td>

                                    <td>{{clients.client_email}}</td>

                                    <td>{{clients.client_phone}}</td>

                                </tr>

                                {{/each}}

                            </div>

                        </tbody>

                    </table>

                </div>

        </div>

    </div>